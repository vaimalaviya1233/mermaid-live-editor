{
  "name": "mermaid-live-editor",
  "version": "2.0.67",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "dev:force": "MERMAID_LOCAL=true yarn dev --force",
    "dev:test": "yarn dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --check --cache --plugin-search-dir=. .;eslint --ignore-path .gitignore .",
    "lint:fix": "prettier --write --cache --plugin-search-dir=. .;eslint --fix --ignore-path .gitignore .",
    "format": "prettier --write --cache --plugin-search-dir=. .",
    "pre-commit": "lint-staged",
    "postinstall": "husky install && svelte-kit sync && (git config blame.ignoreRevsFile .git-blame-ignore-revs || true)",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:browser": "cypress run",
    "test": "test:unit && test:browser",
    "cy": "cypress open"
  },
  "devDependencies": {
    "@cypress/snapshot": "2.1.7",
    "@sveltejs/adapter-static": "1.0.0",
    "@sveltejs/kit": "1.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/svelte": "3.2.2",
    "@types/pako": "2.0.0",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "@vitest/ui": "^0.25.3",
    "autoprefixer": "^10.4.13",
    "c8": "7.12.0",
    "chai": "^4.3.7",
    "cssnano": "^5.1.14",
    "cy-verify-downloads": "0.1.11",
    "cypress": "12.1.0",
    "cypress-localstorage-commands": "2.2.2",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-es": "4.1.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-postcss-modules": "2.0.0",
    "eslint-plugin-svelte3": "4.0.0",
    "eslint-plugin-tailwindcss": "3.7.1",
    "eslint-plugin-vitest": "^0.0.20",
    "esserializer": "1.3.8",
    "font-awesome": "^4.7.0",
    "husky": "^8.0.2",
    "jsdom": "20.0.3",
    "lint-staged": "13.1.0",
    "node-html-parser": "^6.1.4",
    "postcss": "^8.4.19",
    "postcss-load-config": "4.0.1",
    "prettier": "2.8.1",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "3.55.0",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  },
  "dependencies": {
    "@mermaid-js/mermaid-mindmap": "9.3.0",
    "analytics": "0.8.1",
    "analytics-plugin-plausible": "0.0.6",
    "daisyui": "2.42.1",
    "js-base64": "3.7.3",
    "mermaid": "9.3.0",
    "moment": "2.29.4",
    "monaco-editor": "0.34.1",
    "monaco-mermaid": "1.0.8",
    "pako": "2.1.0",
    "random-word-slugs": "0.1.6",
    "svg-pan-zoom": "3.6.1",
    "uuid": "9.0.0"
  },
  "lint-staged": {
    "*.{ts,svelte,js,css,md,json}": [
      "prettier --plugin-search-dir=. --write",
      "eslint --ignore-path .gitignore "
    ]
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  },
  "engines": {
    "node": ">=16.7"
  }
}
